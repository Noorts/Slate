{
  "kind": "collectionType",
  "collectionName": "project_cards",
  "info": {
    "name": "projectCard",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "header": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "repositoryUrl": {
      "type": "string",
      "required": true
    },
    "infoContent": {
      "model": "content-block",
      "via": "projectCards"
    },
    "cardImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    }
  }
}
